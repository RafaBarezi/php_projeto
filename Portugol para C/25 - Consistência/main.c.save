#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>
#include <conio.h>
#include <locale.h>

/*

Nome do programa: Consistência.
Objetivo: Pedir que o usuário digite um valor dentro de um limite específico. O sistema estará em looping até o valor correto ser digitado
Data Criação: 21/03/2022.

*/

int main()

{
    system("title Consistência");
    system("color 03");
    setlocale(LC_ALL, "Portuguese_Brazil");

    int num;

    do
    {
    printf("Por favor, digite um valor entre 10 e 90 >>> ");
    scanf("%i", &num);
    }

    while(num<=10&&num<90);
    {
             printf("\nVocê digitou um número corretamente!\n");
    }

    return 0;
}
