#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("Aula - Operadores relacionais condicionais\n");

    int iJ, iI;

    // -> Idade jovem e idade idosa

    iJ = 17;
    iI = 65;

    int idade = 0;

    printf("Digite a idade de uma pessoa\n");
    scanf("%i", &idade);

    if(idade <= iJ)
    {
        printf("A idade informada e de uma pessoa jovem.\n");
    }else{
        if(idade >= iI){
            printf("A idade informada e de uma pessoa idosa.\n");
        }else{
            if((idade > iJ)&&(idade < iI)){ 
                printf("A idade informada e de uma pessoa adulta.\n");
            }
        }
    }
    return 0
}
